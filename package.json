{
  "name": "vuejs-wheel",
  "version": "0.0.3",
  "description": "A vuejs ui wheel",
  "main": "dist/index.js",
  "scripts": {
    "start": "npx parcel index.html --no-cache",
    "dev-test": "parcel watch test/* --no-cache & karma start",
    "test": "parcel build test/* --no-cache --no-minify && karma start --single-run",
    "commit": "git-cz",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xingxinglail/vuejs-wheel.git"
  },
  "keywords": [
    "vue",
    "ui"
  ],
  "author": "star",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xingxinglail/vuejs-wheel/issues"
  },
  "homepage": "https://github.com/xingxinglail/vuejs-wheel#readme",
  "dependencies": {
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3",
    "vue-router": "^3.1.2"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.0.0",
    "chai": "^4.2.0",
    "commitizen": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "karma": "^4.2.0",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "^3.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "lint-staged": "^8.0.0",
    "mocha": "^6.2.0",
    "parcel-bundler": "^1.12.3",
    "sass": "^1.22.9",
    "sass-loader": "^7.3.1",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.0.3"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
